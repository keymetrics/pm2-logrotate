{
  "name": "pm2-logrotate",
  "version": "1.3.1",
  "description": "Module to rotate logs of every pm2 application",
  "main": "app.js",
  "dependencies": {
    "moment": "2.10.x",
    "pm2": "latest",
    "pmx": "latest",
    "rolex": "0.0.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Joni SHKURTI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pm2-hive/pm2-logrotate.git"
  },
  "bugs": {
    "url": "https://github.com/pm2-hive/pm2-logrotate/issues"
  },
  "homepage": "https://github.com/pm2-hive/pm2-logrotate",
  "license": "MIT",
  "apps": [
    {
      "name": "pm2-logrotate",
      "script": "app.js",
      "merge_logs": true
    }
  ],
  "config": {
    "interval_unit" : "DD",
    "interval" : "1",
    "max_size" : "10M",
    "retain" : "none",
    "white_list": ["pm2-logrotate"]
  }
}
